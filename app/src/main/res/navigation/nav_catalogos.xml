<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_catalogos"
    app:startDestination="@id/catalogos_fragment">

    <fragment
        android:id="@+id/catalogos_fragment"
        android:name="mx.grupo.tepeyac.mexico.aic.siembra.ui.catalogos.CatalogosFragment"
        android:label="CatalogosFragment"
        tools:layout="@layout/lista_fragment">
        <action
            android:id="@+id/action_catalogosFragment_to_listaRanchos"
            app:destination="@id/lista_ranchos" />
        <action
            android:id="@+id/action_catalogos_fragment_to_listaAreas"
            app:destination="@id/lista_areas" />
        <action
            android:id="@+id/action_catalogos_fragment_to_lista_grupos"
            app:destination="@id/lista_grupos" />
        <action
            android:id="@+id/action_catalogos_fragment_to_listaProductos"
            app:destination="@id/lista_productos" />
    </fragment>
    <fragment
        android:id="@+id/lista_ranchos"
        android:name="mx.grupo.tepeyac.mexico.aic.siembra.ui.catalogos.fragments.ranchos.ListaRanchos"
        android:label="lista_fragment"
        tools:layout="@layout/lista_fragment">
        <action
            android:id="@+id/action_lista_ranchos_to_listaTablas"
            app:destination="@id/lista_tablas" />
        <action
            android:id="@+id/action_lista_ranchos_to_formRancho"
            app:destination="@id/form_rancho" />
    </fragment>
    <fragment
        android:id="@+id/lista_tablas"
        android:name="mx.grupo.tepeyac.mexico.aic.siembra.ui.catalogos.fragments.tablas.ListaTablas"
        android:label="lista_tablas_fragment"
        tools:layout="@layout/lista_fragment">
        <action
            android:id="@+id/action_lista_tablas_to_lista_ciclos"
            app:destination="@id/lista_ciclos" />
        <action
            android:id="@+id/action_lista_tablas_to_form_tabla"
            app:destination="@id/form_tabla" />
    </fragment>
    <fragment
        android:id="@+id/lista_ciclos"
        android:name="mx.grupo.tepeyac.mexico.aic.siembra.ui.catalogos.fragments.ciclos.ListaCiclos"
        android:label="ListaCiclos"
        tools:layout="@layout/lista_fragment" >
        <action
            android:id="@+id/action_lista_ciclos_to_from_ciclo"
            app:destination="@id/form_ciclo" />
    </fragment>
    <fragment
        android:id="@+id/lista_areas"
        android:name="mx.grupo.tepeyac.mexico.aic.siembra.ui.catalogos.fragments.areas.ListaAreas"
        android:label="ListaAreas"
        tools:layout="@layout/lista_fragment">
        <action
            android:id="@+id/action_listaAreas_to_listaActividades"
            app:destination="@id/lista_actividades" />
        <action
            android:id="@+id/action_lista_areas_to_form_area"
            app:destination="@id/form_area" />
    </fragment>
    <fragment
        android:id="@+id/lista_actividades"
        android:name="mx.grupo.tepeyac.mexico.aic.siembra.ui.catalogos.fragments.actividades.ListaActividades"
        android:label="ListaActividades"
        tools:layout="@layout/lista_fragment" >
        <action
            android:id="@+id/action_lista_actividades_to_form_actividad"
            app:destination="@id/form_actividad" />
    </fragment>
    <fragment
        android:id="@+id/lista_grupos"
        android:name="mx.grupo.tepeyac.mexico.aic.siembra.ui.catalogos.fragments.grupos.ListaGrupos"
        android:label="ListaGrupos"
        tools:layout="@layout/lista_fragment">
        <action
            android:id="@+id/action_lista_grupos_to_lista_trabajadores"
            app:destination="@id/lista_trabajadores" />
    </fragment>
    <fragment
        android:id="@+id/lista_trabajadores"
        android:name="mx.grupo.tepeyac.mexico.aic.siembra.ui.catalogos.fragments.trabajadores.ListaTrabajadores"
        android:label="ListaTrabajadores"
        tools:layout="@layout/lista_fragment" />
    <fragment
        android:id="@+id/lista_productos"
        android:name="mx.grupo.tepeyac.mexico.aic.siembra.ui.catalogos.fragments.productos.ListaProductos"
        android:label="ListaProductos"
        tools:layout="@layout/lista_fragment" >
        <action
            android:id="@+id/action_lista_productos_to_form_producto"
            app:destination="@id/form_producto" />
    </fragment>
    <fragment
        android:id="@+id/form_rancho"
        android:name="mx.grupo.tepeyac.mexico.aic.siembra.ui.catalogos.fragments.ranchos.form.FormRancho"
        android:label="FormRancho"
        tools:layout="@layout/form_rancho" />
    <fragment
        android:id="@+id/form_tabla"
        android:name="mx.grupo.tepeyac.mexico.aic.siembra.ui.catalogos.fragments.tablas.form.FormTabla"
        android:label="FormTabla"
        tools:layout="@layout/form_tabla" />
    <fragment
        android:id="@+id/form_ciclo"
        android:name="mx.grupo.tepeyac.mexico.aic.siembra.ui.catalogos.fragments.ciclos.form.FormCiclo"
        android:label="FromCiclo"
        tools:layout="@layout/form_ciclo" />
    <fragment
        android:id="@+id/form_producto"
        android:name="mx.grupo.tepeyac.mexico.aic.siembra.ui.catalogos.fragments.productos.form.FormProducto"
        android:label="FormProducto"
        tools:layout="@layout/form_producto" />
    <fragment
        android:id="@+id/form_actividad"
        android:name="mx.grupo.tepeyac.mexico.aic.siembra.ui.catalogos.fragments.actividades.form.FormActividad"
        android:label="FormActividad"
        tools:layout="@layout/form_actividad"/>
    <fragment
        android:id="@+id/form_area"
        android:name="mx.grupo.tepeyac.mexico.aic.siembra.ui.catalogos.fragments.areas.form.FormArea"
        android:label="FormArea"
        tools:layout="@layout/form_area" />
</navigation>